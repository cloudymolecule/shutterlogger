<div id="show">
    <%if flash[:message] %>
    <p><%= flash[:message] %></p>
    <% end %>
    <h3 id="log">Photos taken with: <%="#{@photos[0].camera.make} / #{@photos[0].camera.model}"%></h3><br />
    <% @photos.each do |photo| %>
        <a id="showmenu" href="/photos/<%= photo.id %>"><%= photo.name %><%= " - #{photo.rating} star rating" %> </a><br /><br />
    <% end %><br /><br />
    <a href="/users/<%=current_user.id%>" id="showmenu">Home</a>
</div><br /><br />
